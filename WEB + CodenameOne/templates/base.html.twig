                    </li>

                    <li>
                        <a href="{{ path('app_reclamation_index') }}" class="waves-effect"><i class="mdi mdi-calendar"></i> <span> Réclamations</span> </a>
                    </li>

                    <li>
                        <a href="{{ path('app_reclamation_stats') }}" class="waves-effect"><i class="mdi mdi-email"></i><span> Statistiques </span></a>
                    </li>

                     <li>
                        <a href="{{ path('app_logout') }}" class="waves-effect"><i class="fa fa-sign-out"></i><span>Se déconnecter</span></a>
                    </li>

                </ul>
                <div class="clearfix"></div>
            </div>
            <!-- Sidebar -->
            <div class="clearfix"></div>

        </div>

    </div>
    <!-- Left Sidebar End -->


            <!-- ============================================================== -->
            <!-- Start right Content here -->
            <!-- ============================================================== -->
            <div class="content-page">
